/*
 *= require reset
 *= require_self
 *= require fancybox
 */

body {
  background-image:url(<%=asset_path "cream_dust.png"%>);
  font-family: "Century Gothic", CenturyGothic, AppleGothic, sans-serif;
  font-size:13px;
}

.hidden_header{
background-image:url(<%=asset_path "chicago3.png"%>);
position:fixed;
top:0;
width:100%;
z-index:2;
vertical-align:top;
padding:0px 0px 0px 0px;
margin: 0px auto;
box-shadow: 0px 3px 10px 0px #8b8989;
}


.hidden_header_content{
vertical-align:top;
width:940px;
padding:0px 0px 0px 30px;
margin: 0px auto;
}

.hidden_header_title{
display:inline-block;
margin: 7px 0px 0px 0px;

}

header {
background-image:url(<%=asset_path "chicago2.png"%>);
background-repeat: none;
position:relative;
width:100%;
box-shadow: 0px 0px 10px 5px #8b8989;
z-index:3;
padding:0px 0px 0px 0px;
margin: 0px auto;
}

.header_content{
width:940px;
padding:0px 0px 30px;
margin: 0px auto;
}


.header_content_title{
display:inline-block;
margin: 8px 0px; 8px 0px;
position:relative;
top: 40px;
left: 500px;

}

.hidden_logo{
position:relative;
top:0px;
left:0px;
vertical-align:top;
  padding:3px 5px 4px 5px;
  display:inline-block;
}

.logo {
position:relative;
top:0px;
left:0px;
vertical-align:top;
  padding:6px 10px 5px 10px;
  display:inline-block;
}


h1 {
  display:inline-block;
  font-size:14px;
  font-family: verdana
}

h1 a{
  color: rgba(150, 129, 70, 0.86);
  font-size:0.7em;

}

h2 {
  display:inline-block;
  font-weight:bold;
  font-size:20px;
  color: rgba(105, 106, 150, 0.89);
}


h2 a{
  color: rgba(112, 101, 150, 0.89);
}


p{
color:#fff
}

p2 {
  letter-spacing:2px;
color: rgba(100, 139, 134, 0.89);
}

p3 {
  letter-spacing:3px;
  font-size:9px;
color:#fff;
}

p4 {
  font-size:40px;
}

a, a:active, a:hover, a:visited {
  text-decoration: none;
}





div#people_list {
width: 940px;
margin:3px auto;
padding:6px 0px 0px 10px;
}


div#people_list p {
display:inline-block;
  margin: 13px 0;
  letter-spacing: 0.5px;
}

div#people_list p {
color: rgba(96, 105, 150, 0.89);
}

div#people_list p a {
  color: rgba(108, 139, 150, 0.89);
}

.all_people{
display:inline-block;
}


.video_row {
  display:inline-block;
  text-align:center;
  padding: 5px 10px 0px 30px;
}

.video {
  position:relative;
  background:rgba(139, 137, 137, .2);
  margin: 22px 12px;
  padding: 6px 6px 8px 6px;
  display:inline-block;
  color: #fff;
  }

.video a {
  color: #fff;
}

.video img {
  max-height: 320px;
  max-width: 320px;
  display:block;
}

.video .overlay {
  background-color:rgba(190, 36, 30, .0);
  bottom: 0px;
  display: block;
  left: 6px;
  position: absolute;
  right: 0;
  top: 6px;
  z-index: 1;
  height:180px;
  width:320px;
  -moz-transition-property: background-color;
  -moz-transition-duration: .5s;
  -webkit-transition-property: background-color;
  -webkit-transition-duration: .5s;
  transition-property: background-color;
  transition-duration: .5s;
}

.video .overlay:hover {
  background-color: rgba(48, 75, 211, 0.68);
}

.overlay_copy {
  width:100%;
  height:120px;
  padding:50px 0px 0px 0px;
  opacity:0;
  -moz-transition-property: opacity;
  -moz-transition-duration: .5s;
  -webkit-transition-property: opacity;
  -webkit-transition-duration: .5s;
  transition-property: opacity;
  transition-duration: .5s;
}

.overlay_copy p {
  padding:0px 0px 2px 0px;
  letter-spacing: 1px;
  font-size: 1em;

}

.overlay_copy:hover {
  opacity:1.0;
}

.overlay_user {
  font-size:40px;
  text-align:center;
  width:100%;
  height:100%;
  color:rgba(255, 255, 255, .8);
  letter-spacing:2px;
}

.video_title {
margin:12px 0px 0px 2px;
text-align:left;
  min-height: 20px;
line-height:1.1em;
width:318px;
overflow:hidden;
white-space:nowrap;
  font-size:15px;
  color: rgb(107, 106, 150);
}

.video_title p{
  color: rgb(116, 114, 150);
}


.specific_user{
}

.flash{
margin:20px auto;
text-align:center;
width:200px;
}
